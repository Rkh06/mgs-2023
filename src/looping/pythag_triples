n=int(input("Please enter a number "))
for b in range (1,n):
    for a in range (1,b):
        for c in range (1,1000):
            if (a*a+b*b==c*c):
                    print (b,a,c)

import math
n=int(input("Please enter a positive integer. "))
for b in range (1,n+1):
    for a in range (1,b):
        c= math.sqrt(a*a+b*b)
        d=round(c)
        if c==d:
            print (b,a,d)
            